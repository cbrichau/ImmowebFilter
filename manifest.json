{
	"name": "Immoweb Filter",
  "description": "__MSG_appDesc__",
  "default_locale": "fr",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "favicon-32x32.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["https://www.immoweb.be/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["storage"]
}